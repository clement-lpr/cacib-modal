{
  "name": "cacib",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cacib/cacib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/cacib/cacib.esm.js",
      "require": "./dist/cacib/cacib.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watchAll",
    "test.e2e": "stencil test --e2e --headed",
    "test.all": "npm run test && npm run test.e2e",
    "storybook": "storybook dev -p 6006 --no-open",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-stencil-runner": "^0.0.9",
    "postcss": "^8.5.6",
    "puppeteer": "^24.3.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "storybook-addon-stencil": "^0.2.3",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/sass": "^3.2.3",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.3",
    "storybook": "^10.0.7"
  }
}
